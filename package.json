{
  "name": "phonix-js",
  "description": "A micro ajax http library with a unified api for browsers and nodejs",
  "version": "0.0.3",
  "repository": "https://github.com/virgel1995/phonix",
  "homepage": "http://clony.vercel.app/doc/phonix/",
  "keywords": [
    "ajax",
    "http",
    "nodejs",
    "client",
    "express",
    "https",
    "nust.js",
    "expressjs",
    "phonix",
    "http-2",
    "server-side",
    "server"
  ],
  "browser": "./browserIndex.js",
  "scripts": {
    "build": "grunt build",
    "test": "NODE_ENV=test node node_modules/.bin/mocha \"test/testserver.js\"",
    "test-coverage": "NODE_ENV=test node node_modules/.bin/mocha --require blanket -R html-cov > coverage.html",
		"format": "prettier .",
		"dev": "node test/phonix.js"
  },
  "devDependencies": {
    "blanket": "1.2.3",
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "connect-multiparty": "^2.2.0",
    "express": "^4.18.2",
    "grunt": "^1.5.3",
    "grunt-banner": "0.6.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-uglify": "3.1.0",
    "grunt-shell": "2.1.0",
    "jsdom": "^20.0.3",
    "jshint-stylish": "2.2.1",
    "mocha": "^10.1.0",
    "morgan": "^1.10.0",
    "prettier": "^2.8.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "browser/**/*.js"
      ]
    }
